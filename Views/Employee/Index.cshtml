@model List<CrudDemo.Models.Employee>   //why not AddEmployee
@{
}

<h1>Employee Information</h1>
<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Role</th>
            <th>Phone Number</th>
            <th>Salary</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach(var employee in Model)
{
                <tr>
                    <td>@employee.FirstName</td>
                    <td>@employee.LastName</td>
                    <td>@employee.Role</td>
                    <td>@employee.PhoneNum</td>
                    <td>@employee.Salary</td>
                    <th><a href="Employee/View/@employee.Id">Views</a></th>
                </tr>
}
    </tbody>
</table>
